<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearLayout01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:background="@drawable/relative_khung"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/backBtn"
                android:layout_width="51dp"
                android:layout_height="45dp"
                android:layout_marginLeft="5dp"
                android:background="@drawable/baseline_arrow_back_ios_24"
                android:clickable="true"
                android:focusable="true" />

            <TextView
                android:id="@+id/Textview1"
                android:layout_width="220dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:textColor="@color/white"
                android:text="Thông tin người dùng"
                android:textSize="20sp"
                android:textStyle="bold" />
        </LinearLayout>

        <TextView
            android:id="@+id/textViewCaNhanLabel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:text="Thông tin cá nhân:"
            android:background="@drawable/relative_khung"
            android:textSize="24sp"
            android:gravity="center"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:layout_marginBottom="12dp" />

        <!-- Họ và Tên -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/relative_background_thongtincanhan"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="12dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Họ và Tên:"
                android:textStyle="bold"
                android:textSize="20dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewHoVaTen12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:textSize="20dp"
                android:text="Không có dữ liệu" />
        </LinearLayout>

        <!-- Số điện thoại -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/relative_background_thongtincanhan"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="12dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Số điện thoại:"
                android:textStyle="bold"
                android:textSize="20dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewSDT12"
                android:layout_width="wrap_content"
                android:textColor="@color/white"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:textStyle="bold"
                android:text="Không có dữ liệu" />
        </LinearLayout>

        <!-- Địa chỉ -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/relative_background_thongtincanhan"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="12dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Địa chỉ:"
                android:textStyle="bold"
                android:textSize="20dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewDiaChi12"
                android:layout_width="wrap_content"
                android:textColor="@color/white"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:textStyle="bold"
                android:text="Không có dữ liệu" />
        </LinearLayout>

        <!-- Khoa -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/relative_background_thongtincanhan"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="12dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Khoa:"
                android:textStyle="bold"
                android:textSize="20dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewKhoa12"
                android:layout_width="wrap_content"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:text="Không có dữ liệu" />
        </LinearLayout>

        <!-- Lớp -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/relative_background_thongtincanhan"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="12dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Lớp:"
                android:textStyle="bold"
                android:textSize="20dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewLop12"
                android:layout_width="wrap_content"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:text="Không có dữ liệu" />
        </LinearLayout>

        <!-- Mã sinh viên -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/relative_background_thongtincanhan"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="12dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Mã sinh viên:"
                android:textStyle="bold"
                android:textSize="20dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewMSV12"
                android:layout_width="wrap_content"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:text="Không có dữ liệu" />
        </LinearLayout>

        <!-- Ngành -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/relative_background_thongtincanhan"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="12dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Ngành:"
                android:textStyle="bold"
                android:textSize="20dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewNganh12"
                android:layout_width="wrap_content"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:text="Không có dữ liệu" />
        </LinearLayout>

        <!-- Thông tin tổng quan -->
        <TextView
            android:id="@+id/textViewTongQuanLabel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:text="Thông tin tổng quan:"
            android:background="@drawable/relative_khung"
            android:textSize="24sp"
            android:gravity="center"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:layout_marginBottom="12dp" />

        <!-- Chứng chỉ -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/relative_background_thongtincanhan"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="12dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Chứng chỉ:"
                android:textStyle="bold"
                android:textSize="20dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewChungChi12"
                android:layout_width="138dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/white"
                android:inputType="textMultiLine"
                android:scrollHorizontally="false"
                android:singleLine="false"
                android:textStyle="bold"
                android:text="Không có dữ liệu"
                android:textSize="20dp" />
        </LinearLayout>

        <!-- Kỹ năng -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/relative_background_thongtincanhan"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="12dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kỹ năng:"
                android:textStyle="bold"
                android:textSize="20dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewKyNang12"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/white"
                android:inputType="textMultiLine"
                android:scrollHorizontally="false"
                android:singleLine="false"
                android:textStyle="bold"
                android:text="Không có dữ liệu"
                android:textSize="20dp" />
        </LinearLayout>

        <!-- Kinh nghiệm -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/relative_background_thongtincanhan"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="12dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kinh nghiệm:"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewKinhNghiem12"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/white"
                android:inputType="textMultiLine"
                android:scrollHorizontally="false"
                android:singleLine="false"
                android:textStyle="bold"
                android:text="Không có dữ liệu"
                android:textSize="20dp" />
        </LinearLayout>

        <!-- Liên hệ -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/relative_background_thongtincanhan"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="12dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Liên hệ:"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewLienHe12"
                android:layout_width="115dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/white"
                android:inputType="textMultiLine"
                android:scrollHorizontally="false"
                android:singleLine="false"
                android:textStyle="bold"
                android:text="Không có dữ liệu"
                android:textSize="20dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/relative_background_thongtincanhan"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="12dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="BHYT ID:"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewBHYT12"
                android:layout_width="125dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/white"
                android:inputType="textMultiLine"
                android:scrollHorizontally="false"
                android:singleLine="false"
                android:textStyle="bold"
                android:text="Không có dữ liệu"
                android:textSize="20dp" />
        </LinearLayout>





    </LinearLayout>
</ScrollView>
