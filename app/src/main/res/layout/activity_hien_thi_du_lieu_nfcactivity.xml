<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/BackButton201"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:layout_marginLeft="10dp"
                app:srcCompat="@drawable/baseline_arrow_back_24" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/btnClick"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:layout_marginLeft="30dp"
                app:srcCompat="@drawable/baseline_checklist_24" />

        </LinearLayout>
        <!-- Thông tin cá nhân -->
        <TextView
            android:id="@+id/textViewTokenLabel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Mã định danh:"
            android:textSize="18sp"
            android:textStyle="bold"
            android:layout_marginBottom="12dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="8dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ID:"
                android:textStyle="bold"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/tvData12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Không có dữ liệu" />
        </LinearLayout>

        <TextView
            android:id="@+id/textViewCaNhanLabel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Thông tin cá nhân:"
            android:textSize="18sp"
            android:textStyle="bold"
            android:layout_marginBottom="12dp" />

        <!-- Họ và Tên -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="8dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Họ và Tên:"
                android:textStyle="bold"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewHoVaTen11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Không có dữ liệu" />
        </LinearLayout>

        <!-- Số điện thoại -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="8dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Số điện thoại:"
                android:textStyle="bold"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewSDT11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Không có dữ liệu" />
        </LinearLayout>

        <!-- Địa chỉ -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="8dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Địa chỉ:"
                android:textStyle="bold"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewDiaChi11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Không có dữ liệu" />
        </LinearLayout>

        <!-- Khoa -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="8dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Khoa:"
                android:textStyle="bold"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewKhoa11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Không có dữ liệu" />
        </LinearLayout>

        <!-- Ngành -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="8dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Ngành:"
                android:textStyle="bold"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewNganh11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Không có dữ liệu" />
        </LinearLayout>

        <!-- Lớp -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="16dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Lớp:"
                android:textStyle="bold"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewLop11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Không có dữ liệu" />
        </LinearLayout>

        <!-- Thông tin tổng quan -->
        <TextView
            android:id="@+id/textViewTongQuanLabel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Thông tin tổng quan:"
            android:textSize="18sp"
            android:textStyle="bold"
            android:layout_marginBottom="12dp" />

        <!-- Kỹ năng -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="8dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kỹ năng:"
                android:textStyle="bold"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewKyNang11"
                android:layout_width="31dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:inputType="textMultiLine"
                android:scrollHorizontally="false"
                android:singleLine="false"
                android:text="Không có dữ liệu"
                android:textColor="#000"
                android:textSize="16sp" />

        </LinearLayout>

        <!-- Kinh nghiệm -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="8dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kinh nghiệm:"
                android:textStyle="bold"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewKinhNghiem11"
                android:layout_width="61dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:inputType="textMultiLine"
                android:scrollHorizontally="false"
                android:singleLine="false"
                android:text="Không có dữ liệu"
                android:textSize="16sp" />
        </LinearLayout>

        <!-- Liên hệ -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="8dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Liên hệ:"
                android:textStyle="bold"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewLienHe11"
                android:layout_width="28dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:inputType="textMultiLine"
                android:scrollHorizontally="false"
                android:singleLine="false"
                android:text="Không có dữ liệu"
                android:textSize="16sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="8dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Chứng chỉ:"
                android:textStyle="bold"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textViewChungChi11"
                android:layout_width="46dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:inputType="textMultiLine"
                android:scrollHorizontally="false"
                android:singleLine="false"
                android:text="Không có dữ liệu"
                android:textSize="16sp" />
        </LinearLayout>

    </LinearLayout>
</ScrollView>